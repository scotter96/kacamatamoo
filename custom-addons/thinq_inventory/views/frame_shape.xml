<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Action Window untuk Frame Shape -->
    <record id="action_frame_shape" model="ir.actions.act_window">
        <field name="name">Frame Shape</field>
        <field name="res_model">thinq.frame.shape</field>
        <field name="view_mode">list,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create your first frame shape
            </p>
        </field>
    </record>

    <!-- Form View untuk Frame Shape -->
    <record id="view_frame_shape_form" model="ir.ui.view">
        <field name="name">thinq.frame.shape.form</field>
        <field name="model">thinq.frame.shape</field>
        <field name="arch" type="xml">
            <form string="Frame Shape">
                <sheet>
                    <group>
                        <field name="frame_shape_name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- List View untuk Frame Shape -->
    <record id="view_frame_shape_tree" model="ir.ui.view">
        <field name="name">thinq.frame.shape.list</field>
        <field name="model">thinq.frame.shape</field>
        <field name="arch" type="xml">
            <list string="Frame Shapes">
                <field name="frame_shape_name"/>
            </list>
        </field>
    </record>
</odoo>