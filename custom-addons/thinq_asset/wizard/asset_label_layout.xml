<odoo>
    <record id="view_asset_label_layout_wizard" model="ir.ui.view">
        <field name="name">asset.label.layout.wizard.form</field>
        <field name="model">asset.label.layout</field>
        <field name="arch" type="xml">
            <form string="Print Asset Labels">
                <group>
                    <field name="custom_quantity" string="Custom Quantity"/>
                    <field name="print_format"/>
                </group>
                <footer>
                    <button string="Print" type="object" name="action_print_labels" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_asset_label_layout_wizard" model="ir.actions.act_window">
        <field name="name">Print Asset Labels</field>
        <field name="res_model">asset.label.layout</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
        <field name="context">{'default_asset_ids': active_ids}</field>
    </record>
</odoo>