<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="url_shortener_view_list" model="ir.ui.view">
            <field name="name">url.shortener.view.list</field>
            <field name="model">url.shortener</field>
            <field name="arch" type="xml">
                <list string="URL Shortener" create="0" delete="0" edit="0">
                    <field name="name"/>
                    <field name="original_url"/>
                    <field name="click_count"/>
                </list>
            </field>
        </record>

        <record id="url_shortener_action" model="ir.actions.act_window">
            <field name="name">URL Shortener</field>
            <field name="res_model">url.shortener</field>
            <field name="view_mode">list,form</field>
        </record>

        <menuitem
            id="url_shortener_menu"
            name="URL Shortener"
            action="url_shortener_action"
            groups="base.group_no_one"
            parent="resource.menu_resource_config"
            sequence="10"/>
    
    </data>
    

</odoo>
